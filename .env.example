# ElevaForge Backend Environment Variables
# Copy this file to .env and fill in your values

# Server
PORT=3001
NODE_ENV=development

# Allowed origins (comma-separated) - your frontend URLs
ALLOWED_ORIGINS=http://localhost:3000,https://elevaforge.com,https://www.elevaforge.com

# Supabase (server-side only - NEVER expose service role key to frontend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
